{
	"schemaVersion": 1,
	"id": "mysecondmod",
	"version": "${version}",
	"name": "MySecondMod",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/mysecondmod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.github.fdh911.MySecondMod",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "com.github.fdh911.MySecondModClient",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"mysecondmod.mixins.json",
		{
			"config": "mysecondmod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}